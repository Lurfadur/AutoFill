<Window x:Class="AutoFill.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoFill"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="UserNameLabel" Content="Name:" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.068,-0.615"/>
        <TextBox x:Name="UserNameData" HorizontalAlignment="Left" Height="23" Margin="135,29,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156"/>
        <Label x:Name="DateLabel" Content="Date:" HorizontalAlignment="Left" Margin="10,75,0,0" VerticalAlignment="Top"/>
        <DatePicker x:Name="SelectDateBox" HorizontalAlignment="Left" Margin="135,77,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="OrderNumberLabel" Content="Order Number:" HorizontalAlignment="Left" Margin="10,122,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="OrderNumberData" HorizontalAlignment="Left" Height="23" Margin="135,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
